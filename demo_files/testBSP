AutoBSP{
  controller: STM32F446RE
  //name This is a test line comment
  /*pin This is a /* multi-layered // test comment
      block*/ pushpull*/
  
  OUTPUT{
    name:    "LED_1"
    pin:     PA5;  // Parameter assignments can be ended with a optional semicolon
    type:    pushpull
    pull:    none;
    speed:   medium
    
    init:    off
    active:  high
    
    enable:  true;
  }
  
  OUTPUT{
    name: "LED_2"
    pin:   PC5
    enable: false
  }
  
  INPUT{
    name:    "BUTTON_1"
    pin:     PC13
    pull:    none
    
    active:  low
    
    enable:  true
  }
  
  INPUT{
    name: "BUTTON_2"
    pin:   PB12
    enable: false
  }
  
  PWM_OUTPUT{
    name:     "PWM_1"
    pin:       PA6
    pull:      none
    speed:     medium
    frequency: 1000  // in Hz
    duty:      0     // initial duty cycle in permille (0..1000)
    active:    high
    enable:    true
  }
  
  PWM_OUTPUT{
    name:     "PWM_2"
    pin:       PA1
    pull:      none
    speed:     low
    frequency: 2000  // in Hz
    duty:      0     // initial duty cycle in permille (0..1000)
    active:    low
    enable:    true
  }
}
