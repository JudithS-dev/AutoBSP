AutoBSP{
  controller: STM32F446RE
  //name This is a test line comment
  /*pin This is a /* multi-layered // test comment
      block*/ pushpull*/
  
  OUTPUT{
    name:    "LED_1"
    pin:     PA12;  // Parameter assignments can be ended with a optional semicolon
    type:    pushpull
    pull:    none;
    speed:   medium
    
    init:    on
    active:  high
    
    enable:  true;
  }
  
  INPUT{
    name:    "BUTTON_1"
    pin:     PC13
    pull:    up
    
    active:  low
    
    enable:  true
  }    
}